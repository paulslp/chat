<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="435.0" prefWidth="264.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.geekbrains.chat.client.ChatController">
  <MenuBar VBox.vgrow="NEVER">
    <Menu mnemonicParsing="false" text="File">
      <MenuItem mnemonicParsing="false" text="Quit"/>
    </Menu>
    <Menu mnemonicParsing="false" text="Help">
      <MenuItem mnemonicParsing="false" text="About MyHelloApp"/>
    </Menu>
  </MenuBar>
  <AnchorPane VBox.vgrow="ALWAYS">
    <ListView fx:id="listView" prefHeight="368.0" prefWidth="264.0"/>
    <Button onAction="#onClickMethod" alignment="CENTER" layoutX="14.0" layoutY="368.0" mnemonicParsing="false"
            prefHeight="39.0" prefWidth="234.0" text="Скопировать"/>
  </AnchorPane>
</VBox>
